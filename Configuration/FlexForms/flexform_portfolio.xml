
<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>

	<sheets>
		<!--
			################################
			  SHEET General Settings
			################################
		-->
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>Settings</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<!-- View -->
					<switchableControllerActions>
						<TCEforms>
							<label>General</label>
							<config>
								<type>select</type>
								<items>
									<numIndex index="1">
										<numIndex index="0">Project List and Show</numIndex>
										<numIndex index="1">Project->list;Project->show;Project->showMedia</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</switchableControllerActions>

					<!-- startingpoint  -->
					<settings.startingpoint>
						<TCEforms>
							<label>Category</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>0</show_thumbs>
							</config>
						</TCEforms>
					</settings.startingpoint>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>